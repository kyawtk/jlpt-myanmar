{
  "root": true,
  "extends": ["eslint:recommended", "plugin:prettier/recommended", "next"],
  "plugins": ["prettier", "unused-imports"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "no-unused-vars": "off",
    "unused-imports/no-unused-imports": "error"
  }
}
